# .bashrc

# User specific aliases and functions

# Source global definitions
if [ -f /etc/bashrc ]; then
	. /etc/bashrc
fi

# I want core-dumps dammit
ulimit -S -c unlimited

# exports
export CVS_RSH=ssh
export CVSEDITOR=emacs

# Setup history control
# don't remember all navigation stuff
# or files played with mplayer or mpg123
HISTIGNORE="ls*:rm*:cd*:mpg123*:mplayer*"
export HISTIGNORE
